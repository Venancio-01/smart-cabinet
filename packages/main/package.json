{
  "name": "smart-cabinet",
  "version": "1.0.0",
  "main": "index.js",
  "homepage": "http://www.baidu.com",
  "description": "智能柜管理系统",
  "scripts": {
    "app:start": "sudo node node_modules/.bin/electron . --no-sandbox",
    "app:dev:build": "./script/env.development.sh && tsc -p ./electron",
    "app:prod:build": "./script/env.production.sh && tsc -p ./electron",
    "app:dev": "npm-run-all app:dev:build app:start",
    "gen-db": "rm -rf electron/database/entities & npx typeorm-model-generator -h localhost -d double_door -p 3306 -u sa -x js123456 -e mysql -o electron/database/entities --noConfig true --ce pascal --cp camel",
    "build": "npm-run-all vite:build app:prod:build",
    "package": "npm-run-all build && electron-builder --linux",
    "publish": "node node_modules/.bin/publish"
  },
  "keywords": [
    "electron",
    "vue"
  ],
  "author": "liqingshan <15054854614@163.com>",
  "license": "ISC",
  "dependencies": {
    "electron": "^12.2.2",
    "@prisma/client": "^4.9.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "electron-log": "^4.4.8",
    "electron-simple-updater": "^2.0.11",
    "electron-updater": "^5.3.0",
    "ffi-napi": "^4.0.3",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.3",
    "prisma": "^4.9.0",
    "ref-array-di": "^1.2.2",
    "ref-napi": "^3.0.3",
    "ref-struct-di": "^1.1.1",
    "serialport": "^10.5.0"
  }
}
