generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://sa:js123456@localhost:3306/double_door?schema=public"
}

model doc_document {
  DOC_ID                 Int       @id @default(autoincrement())
  BIND_USER_ID           Int?
  BINDING_DEPT_ID        Int?
  BINDING_ID             Int?
  BINDINGTYPE            Int?
  BUREAU_DEPID           Int?
  CABINET_DOOR_ID        Int?
  CABINET_ID             Int?
  CARRIERTYPE            Int?
  CODE                   String?   @db.VarChar(255)
  CONFIDENTIALITY_PERIOD Int?
  CREATE_TIME            DateTime  @default(dbgenerated("'0000-00-00 00:00:00'")) @db.Timestamp(0)
  DEVICE_BRAND           String?   @db.VarChar(255)
  DEVICE_CAPACITY        String?   @db.VarChar(255)
  DEVICE_MODEL           String?   @db.VarChar(255)
  DEVICE_PURPOSE         String?   @db.VarChar(255)
  DOC_LAST_TIME          DateTime? @db.Timestamp(0)
  DOC_NAME               String    @db.VarChar(255)
  DOC_P_STATUS           Int?
  DOC_RFID               String?   @db.VarChar(255)
  DOC_SECRET_LEVEL       Int?
  DOC_TYPE               Int?
  END_TIME               DateTime? @db.Timestamp(0)
  IS_ONE                 String?   @db.VarChar(255)
  NETWORK_TYPE           Int?
  PAGE_NUMBER            Int?
  PARTICULAR_YEAR        Int?
  PERSON_DUTY            String?   @db.VarChar(255)
  REMARK                 String?   @db.VarChar(255)
  REMOVE_STATUS          Int?
  SEND_CODE              String?   @db.VarChar(255)
  SEND_DATE              DateTime? @db.Timestamp(0)
  SEND_DEP_              String?   @db.VarChar(255)
  SERIAL_NUMBER          Int?
  SHARE_NUMBER           Int?
  START_DATE             DateTime? @db.Timestamp(0)
  STORAGE_LOCATION       String?   @db.VarChar(255)
  UPDATE_TIME            DateTime? @db.Timestamp(0)
  URGENCY                Int?
  USER_ID                Int?
  DEPT_DUTY              String?   @db.VarChar(255)
  DEPTID_DUTY            Int?
  PERSONID_DUTY          Int?
  DOC_RFIDKEY            String?   @db.VarChar(255)
  DOC_REISSUENUMBER      Int
  DOC_REISSUEDATE        DateTime? @db.Timestamp(0)
  DOC_REISSUEFID         Int

  @@map("DOC_DOCUMENT")
}

model doc_user {
  DOC_USER_ID     Int       @id
  USER_ID         Int
  DOC_ID          Int
  TASK_IS         Int
  TAKE_TIMESTART  DateTime? @db.Timestamp(0)
  TAKE_TIMEEND    DateTime? @db.Timestamp(0)
  STILL_IS        Int
  STILL_TIMESTART DateTime? @db.Timestamp(0)
  STILL_TIMEEND   DateTime? @db.Timestamp(0)
  PERSON_DUTY     String?   @db.VarChar(255)
  CREATE_BY       String?   @db.VarChar(255)
  CREATE_BYID     Int
  CREATE_TIME     DateTime? @db.Timestamp(0)

  @@map("DOC_USER")
}

model rfid_cabinet {
  ID                Int     @id
  CABADDR           String? @db.VarChar(255)
  CABINDEX          String? @db.VarChar(255)
  DEPT_ID           Int?
  NAME              String? @db.VarChar(255)
  OPENDOOR          String? @db.VarChar(255)
  OPENBAUDRATE      Int
  ALARM_DURATION    Int
  ALARM_COMBAUDRATE Int
  ALARM_COM         String? @db.VarChar(255)
  SECRETLEVEL       String? @db.VarChar(255)
  CLASSIFICATION    String? @db.VarChar(255)
  STATE             String? @db.VarChar(255)
  TYPECLASS         String? @db.VarChar(255)

  @@map("RFID_CABINET")
}

model rfid_cabinet_door {
  ID          Int     @id
  BINDINGTYPE Int?
  CABINETID   Int
  FOLDERPATH  String? @db.VarChar(255)
  KGBH        Int
  KGZT        Int
  NAME        Int?
