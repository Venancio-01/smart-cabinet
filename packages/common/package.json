{
  "name": "@smart-cabinet/common",
  "type": "module",
  "version": "1.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./system": {
      "types": "./dist/system/index.d.ts",
      "import": "./dist/system/index.js",
      "require": "./dist/system/index.cjs"
    },
    "./activation": {
      "types": "./dist/activation/index.d.ts",
      "import": "./dist/activation/index.js",
      "require": "./dist/activation/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "system": [
        "./dist/system/index.d.ts"
      ],
      "activation": [
        "./dist/activation/index.d.ts"
