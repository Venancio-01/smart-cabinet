  "keywords": [],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "pull-db": "npx prisma db pull",
    "format-db": "cd ./prisma && npx prisma-case-format",
