{
  "name": "database",
  "type": "module",
  "version": "1.0.0",
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "db:pull": "npx prisma db pull",
    "db:format": "npx prisma-case-format --file ./prisma/schema.prisma",
    "db:generate": "npx prisma generate",
    "db": "npm run db:pull && npm run db:format && npm run db:generate",
    "package": "tsup"
  },
  "dependencies": {
    "@prisma/client": "4.16.0",
    "prisma": "4.16.0",
    "prisma-case-format": "^1.7.0"
  },
  "devDependencies": {
    "@types/ffi-napi": "^4.0.7",
    "config": "workspace:^",
    "electron": "^12.2.0",
    "utils": "workspace:^"
  }
}
