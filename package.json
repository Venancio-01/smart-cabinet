{
  "name": "smart-cabinet-repo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "智能载体管控系统",
  "author": "hjrich <15054854614@163.com>",
  "license": "MIT",
  "homepage": "https://www.electronjs.org",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "main": "dist/main/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write .",
    "db": "nx db @smart-cabinet/database",
    "clear": "find ./ -type d -name 'node_modules' -exec rm -rf {} +",
    "up": "npx taze latest",
    "build": "nx run smart-cabinet:build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "js-guid": "^1.0.2",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.19.0",
    "eslint": "^9.3.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  }
}
