{
  "name": "smart-cabinet-repo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "智能载体管控系统",
  "author": "hjrich <15054854614@163.com>",
  "license": "MIT",
  "homepage": "https://www.electronjs.org",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "main": "dist/main/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write .",
    "db": "nx db @smart-cabinet/database",
    "clear": "find ./ -type d -name 'node_modules' -exec rm -rf {} +",
    "up": "npx taze latest",
    "build": "nx run smart-cabinet:build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "js-guid": "^1.0.2",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@smart-cabinet/ui": "workspace:*",
    "@swc/core": "^1.3.105",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "esbuild": "^0.19.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "tsup": "^8.0.1",
    "vite": "^5.0.12",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.3.1",
    "vue-tsc": "^1.8.27"
  }
}
