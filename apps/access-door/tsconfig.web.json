{
  "extends": "@smart-cabinet/config/tsconfig.web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./renderer/*"],
      "~/*": ["./types/*"],
      "#/*": ["./common/*"]
    }
  },
  "include": [
    "renderer/env.d.ts",
    "renderer/**/*",
    "renderer/**/*.vue",
    "renderer/**/*.json",
    "renderer/*.d.ts",
    "types/*.ts",
    "types/*.d.ts"
  ]
}
