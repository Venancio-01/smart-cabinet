generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model doc_document {
  doc_id           BigInt    @id @default(autoincrement())
  doc_name         String    @db.VarChar(200)
  doc_secret_level Int?
  doc_type         Int?      @db.TinyInt
  doc_rfid         String?   @db.VarChar(128)
  remark           String?   @db.VarChar(100)
  doc_p_status     Int?      @default(1)
  user_id          BigInt?
  dept_id          BigInt?
  cabinet_id       Int?      @default(0)
  cabinet_door_id  Int?
  update_time      DateTime? @db.DateTime(0)
  create_time      DateTime  @db.DateTime(0)
  IS_one           String?   @default("0") @db.VarChar(255)
  doc_last_time    DateTime? @db.DateTime(0)
  end_time         DateTime? @db.DateTime(0)
  remove_status    Int?      @default(0) @db.TinyInt
  code             String?   @db.VarChar(255)
  urgency          Int?      @db.TinyInt
  send_dep         Int?      @db.TinyInt
  send_code        String?   @db.VarChar(255)
  bureau_depId     BigInt?
  type             Int?      @db.TinyInt
  carrier_type_id  BigInt?
  receive          String?   @db.VarChar(10)
  year             String?   @db.VarChar(10)
  num              String?   @db.VarChar(50)

  @@index([doc_name], map: "doc_name")
  @@index([doc_rfid], map: "rfid")
}

